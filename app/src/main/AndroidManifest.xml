<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="myfirstapp.goodiefoodie" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconnew"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="myfirstapp.goodiefoodie.SplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="myfirstapp.goodiefoodie.MainScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="myfirstapp.goodiefoodie.CLEARSCREEN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="myfirstapp.goodiefoodie.RiceRecipeList"
            android:label="@string/app_name" >

        </activity>

        <activity
            android:name="myfirstapp.goodiefoodie.CorianderRiceRecipe"
            android:label="@string/app_name" >

        </activity>

    </application>

</manifest>
